{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 59
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/process-bin",
      "dest": "/api/process-bin.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 